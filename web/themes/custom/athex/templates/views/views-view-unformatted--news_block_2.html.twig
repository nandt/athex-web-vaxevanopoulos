{# web/core/themes/stable9/templates/views/views-view-unformatted.html.twig #}

<div class="newsViewTeaserWrapper">
	{% for row in rows %}
		<div class="news-item">
			{{- row.content -}}
		</div>
	{% endfor %}
</div>
<div class="controls news-teaser-controls" id="news-teaser-slider" aria-label="Carousel Navigation" tabindex="0">
	<span class="prev" data-controls="prev" aria-controls="customize" tabindex="-1">
	</span>
	<span class="next" data-controls="next" aria-controls="customize" tabindex="-1">
	</span>
</div>
